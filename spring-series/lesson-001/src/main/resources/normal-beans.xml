<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="beanThree" class="com.series.demo.test.StrongDependenceBean$BeanThree">
        <constructor-arg name="beanTwo" ref="beanTwo"></constructor-arg>
    </bean>

    <bean id="beanTwo" class="com.series.demo.test.StrongDependenceBean$BeanTwo">
        <constructor-arg name="beanOne" ref="beanOne"></constructor-arg>
    </bean>

    <bean id="beanOne" class="com.series.demo.test.StrongDependenceBean$BeanOne"></bean>

</beans>